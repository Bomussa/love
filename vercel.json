{
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "headers": [
    {
      "source": "/api/v1/(.*)",
      "headers": [
        {
          "key": "X-Accel-Buffering",
          "value": "no"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://YOUR-SUPABASE-REF.functions.supabase.co/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}