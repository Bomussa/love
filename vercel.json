{
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "headers": [
    {
      "source": "/api/v1/(.*)",
      "headers": [
        {
          "key": "X-Accel-Buffering",
          "value": "no"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/v1/status",
      "destination": "https://YOUR-SUPABASE-REF.functions.supabase.co/api-v1-status"
    },
    {
      "source": "/api/v1/patient/login",
      "destination": "https://YOUR-SUPABASE-REF.functions.supabase.co/patient-login"
    },
    {
      "source": "/api/v1/queue/enter",
      "destination": "https://YOUR-SUPABASE-REF.functions.supabase.co/queue-enter"
    },
    {
      "source": "/api/v1/queue/status",
      "destination": "https://YOUR-SUPABASE-REF.functions.supabase.co/queue-status"
    },
    {
      "source": "/api/v1/pin/status",
      "destination": "https://YOUR-SUPABASE-REF.functions.supabase.co/pin-status"
    },
    {
      "source": "/api/v1/events/stream",
      "destination": "https://YOUR-SUPABASE-REF.functions.supabase.co/events-stream"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}