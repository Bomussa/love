{
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "headers": [
    {
      "source": "/api/v1/(.*)",
      "headers": [
        {
          "key": "X-Accel-Buffering",
          "value": "no"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/v1/events/stream",
      "destination": "https://rujwuruuosffcxazymit.functions.supabase.co/events-stream"
    },
    {
      "source": "/api/v1/:path*",
      "destination": "https://rujwuruuosffcxazymit.functions.supabase.co/api-router?path=:path*"
    },
    {
      "source": "/qr",
      "destination": "/index.html"
    },
    {
      "source": "/admin",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}